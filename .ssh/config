StrictHostKeyChecking no
#UserKnownHostsFile=/dev/null
#HashKnownHosts no

Host *.github.com
  IdentityFile ~/.ssh/id_rsa_github

Host *.bitbucket.org
  IdentityFile ~/.ssh/id_rsa_bitbucket

Host git.ph.ed.ac.uk
  IdentityFile ~/.ssh/id_rsa_gitlab

Host git.ecdf.ed.ac.uk
  IdentityFile ~/.ssh/id_rsa_gitlab

Host *.ed.ac.uk
  User borourke
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials yes

Host vm-tarantula
  HostName tarantula.ph.ed.ac.uk
  User borourke
  ProxyCommand ssh -W %h:%p dory.ph.ed.ac.uk
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials yes

Host vm-eagle
  HostName eagle.ph.ed.ac.uk
  User borourke
  ProxyCommand ssh -W %h:%p dory.ph.ed.ac.uk
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials yes

Host vm-hayford
  HostName hayford.ph.ed.ac.uk
  User borourke
  ProxyCommand ssh -W %h:%p cumberdale-to-hayford
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials yes

Host cumberdale-to-hayford
  Hostname cumberdale.ph.ed.ac.uk
  User borourke
  ProxyCommand ssh -W %h:%p dory.ph.ed.ac.uk
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials yes

Host vm-tarantula
  HostName tarantula.ph.ed.ac.uk
  User borourke
  ProxyCommand ssh -W %h:%p cumberdale-to-hayford
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials yes
