urxvt*geometry:         120x30
urxvt*externalBorder:   0
urxvt*internalBorder:   0
urxvt*background:       #000000
urxvt*foreground:       #00ff00
urxvt*fading:           15
urxvt*tintColor:        #ffffff
urxvt*shading:          100
urxvt*inheritPixmap:    False
urxvt*scrollBar:        False
urxvt*saveLines:        10000
urxvt*cursorBlink:      true
urxvt*font:             xft:Bitstream Vera Sans Mono:size=10:antialias=true
urxvt*boldfont:         xft:Bitstream Vera Sans Mono:size=10:antialias=true
urxvt*secondaryScroll:  true
urxvt*pastableTabs:     true
urxvt*depth:            32
urxvt*background:       rgba:2121/2121/2121/eeee
urxvt*foreground:       #f0f0f0
urxvt*cursorColor:      #f0f0f0

Xft.dpi:96
Xft.hinting:true
Xft.hintstyle:hintslight

!black text
*color0: #242424
*color8: #cdb5cd

!red text
*color1: #bf7979
*color9: #f4a45f

!green text                                                                      
*color2: #97b26b
*color10: #c5f779

!yellow text
*color3: #cdcda1
*color11: #ffffaf

!blue text
*color4: #86a2be
*color12: #98afd9

!magenta text
*color5: #d9b798
*color13: #d7d998
!
!cyan text
*color6: #a1b5cd
*color14: #a1b5cd

!white
